// Tests lists
$b = [ 1, 2, 3, 4, 5 ];
$c = [ 1, 2, 3, ];
$c1 = [ 1, ];
$c2 = [ 1 ];
$c3 = [ ];
$e = [ 1, 2, [ 3, 4 ], 5, 6, [ 7 ] ];


// Test hashes
$d1 = { 1 : $a, 2 : $b, "c" : $c };
$d2 = { 1 : $a,  };
$d3 = { };
$d4 = { 1 : $a, 2 : $b, "c" : { $a1_1 : $a, 21 : $b, "c_1" : $c }, "d" : 4 };

$d4 = { 1 : $a, 2 : $b, "c" : { $a1_1 : $a, 21 : $b, "c_1" : $c }, "d" : 4, "e" : [ 1, 2, 3, [ 4, 5 ]] };


// assign multiple vars to multiple lvals
$a, $b = $c, $d;

