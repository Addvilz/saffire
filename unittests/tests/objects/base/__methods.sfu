import io;

class myclass {
    public method foo() {}
    public method bar() {}
    public property baz;
}

a = myclass();
foreach (a.__methods() as k,v) {
    io.print(v, "\n");
}
========
bar
foo
@@@@@@
import io;

class myclass extends string {
    public method foo() {}
    public method bar() {}
    public property baz;
}

a = myclass("a string");
foreach (a.__methods() as k,v) {
    io.print(v, "\n");
}
========
bar
foo
